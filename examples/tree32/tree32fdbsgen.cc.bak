#include <iostream>

using namespace std;

int main(int argc, char** argv)
{
    for (int i = 0; i < 4; ++i)
    {
        cout << i << ": 64 ";
        for (int j = 0; j < 64; ++j)
        {
            if (i == j / 16)
            {
                cout << j % 16 << " ";
            }
            else
            {
                cout << "16 ";
            }
        }
        cout << "\n";
    }

    cout << "4: 64 ";
    for (int i = 0; i < 64; ++i)
    {
        cout << i / 16 << " ";
    }
    cout << "\n";
}